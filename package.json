{
  "name": "picoapp",
  "version": "3.6.2",
  "description": "üê£ Tiny no-framework component toolkit.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/picoapp.umd.cjs",
  "module": "./dist/picoapp.js",
  "exports": {
    ".": {
      "import": "./dist/picoapp.js",
      "require": "./dist/picoapp.umd.cjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vitest",
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "test": "vitest run --coverage",
    "prepare": "is-ci || npx husky install",
    "typecheck": "tsc --noEmit",
    "link": "prettier --check .",
    "format": "prettier --write ."
  },
  "keywords": [
    "web components",
    "custom elements"
  ],
  "license": "MIT",
  "repository": "https://github.com/estrattonbailey/picoapp",
  "author": "estrattonbailey",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.4.5",
    "@vitest/coverage-v8": "^0.34.1",
    "commitlint": "^17.6.7",
    "happy-dom": "^10.5.2",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "prettier": "^3.0.0",
    "semantic-release": "^21.0.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "evx": "^1.1.1",
    "martha": "^4.1.3"
  }
}
